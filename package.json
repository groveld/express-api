{
  "name": "express-api",
  "version": "0.0.1",
  "description": "This is a sample server for a pet store",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "dev": "nodemon src/index.js -e js,json,yaml",
    "test": "jest",
    "format:check": "prettier . --check --ignore-path .gitignore",
    "format:write": "prettier . --write --ignore-path .gitignore",
    "lint:check": "eslint . --ext .cjs,.js,.ts --ignore-path .gitignore",
    "lint:write": "eslint . --ext .cjs,.js,.ts --fix --ignore-path .gitignore",
    "style:check": "run-p --continue-on-error --aggregate-output format:check lint:check",
    "style:write": "run-p --continue-on-error --aggregate-output format:write lint:write",
    "pre-commit": "run-s style:check test",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{json,cjs,js}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "@logtail/node": "^0.2.2",
    "@logtail/winston": "^0.3.3",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.0.3",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.2.0",
    "sequelize": "^6.30.0",
    "ulid": "^2.3.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "supertest": "^6.3.3"
  }
}
